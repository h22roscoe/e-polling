<!DOCTYPE HTML>
<html lang="en">
{{> header}}
<body style="background-color:#efefef">
  <div id="checkModal" class="modal">
    <div class="modal-content">
      <div class="modal-title">
        <p>Are you sure you want to delete this box?</p>
      </div>
      <button id="delete-check-yes" class="btn">Yes</button>
      <button id="delete-check-no" class="btn">No</button>
    </div>
  </div>
  <div class="container">
      <div class="row">
          <div class="col s2">
            <a class="btn z-depth-0" href="/">back</a>
          </div>
          <div class="col s6">
              <form>
                  <div class="row">
                      <div class="input-field col s6">
                          <i class="material-icons prefix">email</i>
                          <input id="email" type="text" class="validate">
                          <label for="email">Email</label>
                      </div>
                      <div class="input-field col s6">
                          <i class="material-icons prefix">lock</i>
                          <input id="password" type="password" class="validate">
                          <label for="password">Password (optional)</label>
                      </div>
                  </div>
              </form>
          </div>
      </div>
      <div class="center-align" style="padding:20%" id="boxContainer"></div>
      <div>
        <button class="btn z-depth-0" id="submit-poll">Submit</button>
    </div>
  </div>


  <script>id = 0; list = [];</script>
  <script src="/src/Box.js"></script>
  <script src="/src/create.js"></script>
</body>
</html>
